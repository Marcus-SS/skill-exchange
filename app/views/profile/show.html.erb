
<div class="profile-container">
  <div class="profile-left">
    <%= cl_image_tag current_user.photo.key, height: 300, width: 400, crop: :fill %>
    <p><%= @user.name %></p>
  </div>

  <div class="profile-right">
    <p><%= @user.email %></p>
    <p><%= @user.age %></p>
    <p><%= @user.gender %></p>
    <p><%= @user.bio %></p>
    <p><%= @user.city %></p>
    <p><%= @user.availibility %></p>
  </div>
</div>
